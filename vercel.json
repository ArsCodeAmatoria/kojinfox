{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "devCommand": "npm run dev",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NEXT_PUBLIC_BASE_URL": "https://kojinfox.vercel.app",
    "SKIP_ESLINT": "1",
    "DISABLE_ESLINT_PLUGIN": "true",
    "NEXT_DISABLE_ESLINT": "1"
  }
} 